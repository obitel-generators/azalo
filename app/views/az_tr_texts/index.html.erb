<h1>Listing az_tr_texts</h1>

<table class="table">
  <tr>
    <th>Owner</th>
    <th>Az operation</th>
    <th>Name</th>
    <th>Text</th>
    <th>Data type</th>
    <th>Copy of</th>
  </tr>

<% @az_tr_texts.each do |az_tr_text| %>
  <tr>
    <td><%=h az_tr_text.owner_id %></td>
    <td><%=h az_tr_text.az_operation_id %></td>
    <td><%=h az_tr_text.name %></td>
    <td><%=h az_tr_text.text %></td>
    <td><%=h az_tr_text.data_type %></td>
    <td><%=h az_tr_text.copy_of %></td>
    <td><%= link_to 'Show', az_tr_text %></td>
    <td><%= link_to 'Edit', edit_az_tr_text_path(az_tr_text) %></td>
    <td><%= link_to t(:az_label_delete), az_tr_text, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to t(:az_label_create), new_az_tr_text_path %>