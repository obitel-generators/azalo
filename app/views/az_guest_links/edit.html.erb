<h1>Editing az_guest_link</h1>

<%= form_for(@az_guest_link) do |f| %>
  <%= f.error_messages %>

  <%= render( :partial => 'new_edit', :locals => {:form => f}) %>
  
  <p>
    <%= submit_and_disable_tag(f, t(:az_label_save)) %>
  </p>
<% end %>

<%= link_to 'Show', @az_guest_link %> |
<%= link_to t(:az_label_back), az_guest_links_path %>