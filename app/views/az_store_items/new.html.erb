<h1>Новый товар в магазине</h1>

<% form_for(@az_store_item, :html => { :multipart => true }) do |f| %>
  <%= f.error_messages %>

  <%= render( :partial => 'new_edit', :locals => {:f => f}) %>

  <p>
    <%= f.submit t(:az_label_create) %>
  </p>
<% end %>

<%= link_to 'К списку товаров', az_store_items_path %>