<h1>New az_purchase</h1>

<% form_for(@az_purchase) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :az_company_id %><br />
    <%= f.text_field :az_company_id %>
  </p>
  <p>
    <%= f.label :az_item_store_id %><br />
    <%= f.text_field :az_item_store_id %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', az_purchases_path %>